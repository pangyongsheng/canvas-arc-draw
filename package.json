{
  "name": "drag-arc",
  "version": "1.0.7",
  "description": "可拖拽的园弧形进度条组件（支持移动端）",
  "main": "dist/drag-arc.umd.js",
  "module": "dist/drag-arc.esm.js",
  "unpkg": "dist/drag-arc.min.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build:browser && cross-env NODE_ENV=production npm run build:es && cross-env NODE_ENV=production npm run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "build:cjs": "rollup --config build/rollup.config.cjs.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "rollup --config build/rollup.config.browser.js --watch"
  },
  "author": "pangyongsheng",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "resolve": "^1.17.0",
    "rollup": "^2.17.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-uglify-es": "^0.0.1",
    "rollup-pluginutils": "^2.8.2"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.10.3",
    "core-js": "^3.6.5"
  }
}
